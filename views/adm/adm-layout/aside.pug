// ASIDE
// ===================================================
aside#aside-container
    #aside
        .nano
            .nano-content
                // Nav tabs
                // ================================
                ul.nav.nav-tabs.nav-justified
                    li.active
                        a(href='#demo-asd-tab-1', data-toggle='tab')
                            i.demo-pli-speech-bubble-7.icon-lg
                    li
                        a(href='#demo-asd-tab-2', data-toggle='tab')
                            i.demo-pli-information.icon-lg.icon-fw
                    li
                        a(href='#demo-asd-tab-3', data-toggle='tab')
                            i.demo-pli-wrench.icon-lg.icon-fw
                // ================================
                // End nav tabs
                // Tabs Content
                // ================================
                .tab-content
                    // First tab (Contact list)
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    #demo-asd-tab-1.tab-pane.fade.in.active
                        p.pad-all.text-main.text-sm.text-uppercase.text-bold
                            span.pull-right.badge.badge-warning 3
                        // Family
                        .list-group.bg-trans
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/2.png', alt='Profile Picture')
                                    i.badge.badge-success.badge-stat.badge-icon.pull-left
                                .media-body
                                    p.mar-no.text-main Stephen Tran
                                    small.text-muteds Availabe
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/7.png', alt='Profile Picture')
                                .media-body
                                    p.mar-no.text-main Brittany Meyer
                                    small.text-muteds I think so
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/1.png', alt='Profile Picture')
                                    i.badge.badge-info.badge-stat.badge-icon.pull-left
                                .media-body
                                    p.mar-no.text-main Jack George
                                    small.text-muteds Last Seen 2 hours ago
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/4.png', alt='Profile Picture')
                                .media-body
                                    p.mar-no.text-main Donald Brown
                                    small.text-muteds Lorem ipsum dolor sit amet.
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/8.png', alt='Profile Picture')
                                    i.badge.badge-warning.badge-stat.badge-icon.pull-left
                                .media-body
                                    p.mar-no.text-main Betty Murphy
                                    small.text-muteds Idle
                            a.list-group-item(href='#')
                                .media-left.pos-rel
                                    img.img-circle.img-xs(src='img/profile-photos/9.png', alt='Profile Picture')
                                    i.badge.badge-danger.badge-stat.badge-icon.pull-left
                                .media-body
                                    p.mar-no.text-main Samantha Reid
                                    small.text-muteds Offline
                        hr
                        p.pad-all.text-main.text-sm.text-uppercase.text-bold
                            span.pull-right.badge.badge-success Offline
                        // Works
                        .list-group.bg-trans
                            a.list-group-item(href='#')
                                span.badge.badge-purple.badge-icon.badge-fw.pull-left
                            a.list-group-item(href='#')
                                span.badge.badge-info.badge-icon.badge-fw.pull-left
                            a.list-group-item(href='#')
                                span.badge.badge-success.badge-icon.badge-fw.pull-left
                            a.list-group-item(href='#')
                                span.badge.badge-danger.badge-icon.badge-fw.pull-left
                        hr
                        p.pad-all.text-main.text-sm.text-uppercase.text-bold News
                        .pad-hor
                            p
                                a.add-tooltip.text-semibold.text-main(data-title='45%', href='#') adipiscing elit
                            small
                                em Last Update : Des 12, 2014
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    // End first tab (Contact list)
                    // Second tab (Custom layout)
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    #demo-asd-tab-2.tab-pane.fade
                        // Monthly billing
                        .pad-all
                            p.pad-ver.text-main.text-sm.text-uppercase.text-bold Billing &amp; reports
                            p
                                strong.text-main $5.00
                        hr.new-section-xs
                        .pad-all
                            span.pad-ver.text-main.text-sm.text-uppercase.text-bold Amount Due On
                            p.text-sm August 17, 2018
                            p.text-2x.text-thin.text-main $83.09
                            button.btn.btn-block.btn-success.mar-top Pay Now
                        hr
                        p.pad-all.text-main.text-sm.text-uppercase.text-bold Additional Actions
                        // Simple Menu
                        .list-group.bg-trans
                            a.list-group-item(href='#')
                                i.demo-pli-information.icon-lg.icon-fw
                            a.list-group-item(href='#')
                                i.demo-pli-mine.icon-lg.icon-fw
                            a.list-group-item(href='#')
                                span.label.label-info.pull-right New
                                i.demo-pli-credit-card-2.icon-lg.icon-fw
                            a.list-group-item(href='#')
                                i.demo-pli-support.icon-lg.icon-fw
                        hr
                        .text-center
                            div
                                i.demo-pli-old-telephone.icon-3x
                            p.text-lg.text-semibold.text-main  (415) 234-53454 
                            small
                                em We are here 24/7
                    // End second tab (Custom layout)
