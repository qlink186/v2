                                small.text-muted Lorem ipsum dolor sit amet, consectetuer adipiscing elit.
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-2.toggle-switch(type='checkbox', checked='')
                                    label(for='demo-switch-2')
                                p.mar-no.text-main Show offline contact
                                small.text-muted Aenean commodo ligula eget dolor. Aenean massa.
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-3.toggle-switch(type='checkbox')
                                    label(for='demo-switch-3')
                                p.mar-no.text-main Invisible mode 
                                small.text-muted
                        hr
                        ul.list-group.pad-btm.bg-trans
                            li.list-header
                                p.text-main.text-sm.text-uppercase.text-bold.mar-no Public Settings
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-4.toggle-switch(type='checkbox', checked='')
                                    label(for='demo-switch-4')
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-5.toggle-switch(type='checkbox', checked='')
                                    label(for='demo-switch-5')
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-6.toggle-switch(type='checkbox', checked='')
                                    label(for='demo-switch-6')
                        hr
                        p.pad-hor.text-main.text-sm.text-uppercase.text-bold.mar-no Task Progress
                        .pad-all
                            p.text-main Upgrade Progress
                            .progress.progress-sm
                                .progress-bar.progress-bar-success(style='width: 15%;')
                                    span.sr-only 15%
                            small 15% Completed
                        .pad-hor
                            p.text-main Database
                            .progress.progress-sm
                                .progress-bar.progress-bar-danger(style='width: 75%;')
