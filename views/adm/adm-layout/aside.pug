                    // Third tab (Settings)
                    // ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                    #demo-asd-tab-3.tab-pane.fade
                        ul.list-group.bg-trans
                            li.pad-top.list-header
                                p.text-main.text-sm.text-uppercase.text-bold.mar-no Account Settings
                            li.list-group-item
                                .pull-right
                                    input#demo-switch-1.toggle-switch(type='checkbox', checked='')
